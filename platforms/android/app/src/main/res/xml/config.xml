<?xml version='1.0' encoding='utf-8'?>
<widget id="io.calculator.LoanEMI" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PollfishPlugin">
        <param name="android-package" value="com.pollfish.cordova.PollfishPlugin" />
    </feature>
    <name>Loan EMI Calculator</name>
    <description>
        A simple financial calculator for all your Loan Calculation and EMI revealer.
    </description>
    <author email="queries@keshavsharma.ml" href="www.keshavsharma.ml">
        InnovativeDevelopers
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
